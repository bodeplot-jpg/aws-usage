{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": [
        "*"
      ]
    },
    {
      "Effect": "Deny",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": [
        "*"
      ],
      "Condition" : {
        "StringNotEquals": {
          "aws:SourceVpce": "${aws_vpc_endpoint}"
        }
      }
    }
  ]
}